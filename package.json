{
  "name": "gotest-runner",
  "version": "0.0.1",
  "description": "A jest runner to run go tests",
  "main": "build/index.js",
  "author": "Maxime Heckel (@MaximeHeckel)",
  "license": "MIT",
  "scripts": {
    "build": "babel src --ignore **/*.spec.js,integration -d build",
    "lint": "eslint .",
    "watch": "babel src -w --ignore **/*.spec.js,integration -d build",
    "test": "jest"
  },
  "dependencies": {
    "eslint": "^4.9.0",
    "throat": "^4.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-flow": "^6.23.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.3.2",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "execa": "^0.8.0",
    "flow-bin": "^0.57.3",
    "jest": "^21.2.1"
  }
}
